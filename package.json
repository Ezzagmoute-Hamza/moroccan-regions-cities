{
  "name": "moroccan-regions-cities",
  "version": "2.0.0",
  "description": "A comprehensive, type-safe Node.js package for handling Moroccan regions and cities with advanced features including caching, validation, search functionality, and multi-language support.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ezzagmoute-Hamza/moroccan-regions-cities.git"
  },
  "bugs": {
    "url": "https://github.com/Ezzagmoute-Hamza/moroccan-regions-cities/issues"
  },
  "homepage": "https://github.com/Ezzagmoute-Hamza/moroccan-regions-cities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run validate && npm run build",
    "clean": "rm -rf dist",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "morocco",
    "moroccan",
    "regions",
    "cities",
    "provinces",
    "administrative-divisions",
    "geography",
    "location",
    "typescript",
    "node",
    "npm-package",
    "multilingual",
    "search",
    "validation",
    "caching",
    "type-safe",
    "data-integrity",
    "utilities",
    "configuration"
  ],
  "author": {
    "name": "Ezzagmoute Hamza",
    "email": "ezzagmoute.hamza@gmail.com",
    "url": "https://github.com/Ezzagmoute-Hamza"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.8.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}